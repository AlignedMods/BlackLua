// Should return 10
int While() {
    int i = 0;
    while (i < 10) {
        i += 1;
    }

    return i;
}

// Should return 10
int DoWhile1() {
    int i = 0;
    do {
        i += 1;
    } while (i < 10);

    return i;
}

// Should return true
// True means that the loop was ran before the condition was checked, which is the correct behavior
bool DoWhile2() {
    do {
        return true;
    } while (false);

    return false;
}

bool If() {
    if (4 < 6) {
        return false;
    }

    return true;
}